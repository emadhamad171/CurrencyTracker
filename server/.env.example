# Порт для сервера
PORT=5000

# Настройки базы данных MongoDB
MONGODB_URI=mongodb://localhost:27017/currency_tracker

# API ключи для сервисов курсов валют
FIXER_API_KEY=your_fixer_api_key
OPEN_EXCHANGE_RATES_API_KEY=your_openexchangerates_api_key
CURRENCYLAYER_API_KEY=your_currencylayer_api_key

# Окружение
NODE_ENV=development

# Интервал обновления курсов (в миллисекундах)
RATES_UPDATE_INTERVAL=3600000

# Настройки кэширования
CACHE_TTL=3600
